{
  "getter/setter": {
    "prefix": ["~gs"],
    "description": "class getter and setter",
    "body": [
      "get $1(): $2 {",
      "\treturn this.$3;",
      "}",
      "",
      "set $1(value: $2) {",
      "\tthis.$3 = value;",
      "}"
    ]
  },
  "eslint-disable-line": {
    "prefix": ["~edl"],
    "description": "insert eslint-disable-line comment",
    "body": [
      "// eslint-disable-line"
    ]
  },
  "eslint-disable-next-line": {
    "prefix": ["~ednl"],
    "description": "insert eslint-disable-next-line comment",
    "body": [
      "// eslint-disable-next-line"
    ]
  }
}
